{
    "name": "Secplugs-Chrome Plugin",
    "version": "1.0",
    "description": "Scan URLs and warn user if its malicious",
    "manifest_version": 2,
    "icons": {
        "32": "logo.png"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
              "modules/jquery-3.5.0.min.js",              
              "content.js"
            ]
        }
    ],    
    "background": {      
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_title": "Click here to open menu",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "storage"
    ],
    "web_accessible_resources": [
        "modules/*",
        "background.html"
    ]
}