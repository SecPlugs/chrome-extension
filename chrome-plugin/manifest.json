{
    "name": "Secplugs-Chrome Plugin",
    "version": "1.0",
    "description": "Scan URLs and warn user if its malicious",
    "manifest_version": 2,
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
              "modules/jquery-3.5.0.min.js",              
              "content.js"
            ]
        }
    ],    
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "modules/*"
    ]
}