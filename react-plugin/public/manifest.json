{
  "name": "Secplugs Chrome Plugin",
  "version": "1.0",
  "description": "Scan URLs and warn user if its malicious",
  "manifest_version": 2,
  "icons": {      
      "32": "logo.png"      
  },   
  "background": {      
      "page": "apps/background.html"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>"
  ],
  "web_accessible_resources": [
      "modules/*"
  ],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}